div(class='row')
  div(class='col-md-4', ng-repeat='image in images')
    img(ng-src='{{image.url}}', class='img-responsive')
div(class='row')
  div(class='col-md-12')
    form(ng-submit='processForm()')
      input(type='text', class='form-control', ng-model='formData.title')
      textarea(class='form-control form-textarea', ng-model='formData.text')
      input(type='submit', class='btn btn-primary pull-right')
